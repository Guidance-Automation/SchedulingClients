<Project Sdk="Microsoft.NET.Sdk">

	<PropertyGroup>
		<TargetFramework>net8.0</TargetFramework>
		<PackageVersion>17.0.0-rc.1</PackageVersion>
		<Platforms>x64</Platforms>
		<ImplicitUsings>true</ImplicitUsings>
		<Nullable>enable</Nullable>
		<Authors>Martin Davies, Callum Clarke, Ollie Sharratt</Authors>
		<Copyright>Guidance Automation 2024</Copyright>
		<Description>Scheduling client components for Guidance Automation schedulers.</Description>
		<RepositoryUrl>https://github.com/GuidanceAutomation/FleetClients</RepositoryUrl>
		<PackageId>SchedulingClients</PackageId>
		<Company>Guidance Automation LTD</Company>
		<GeneratePackageOnBuild>true</GeneratePackageOnBuild>
		<PackageLicenseExpression>MIT</PackageLicenseExpression>
		<PackageReadmeFile>README.md</PackageReadmeFile>
	</PropertyGroup>

	<ItemGroup>
		<PackageReference Include="GAAPICommon" Version="2.0.*-*" />
		<PackageReference Include="Grpc.Net.Client" Version="2.64.*" />
		<PackageReference Include="Grpc.Net.ClientFactory" Version="2.64.*" />
		<PackageReference Include="protobuf-net.Grpc" Version="1.1.*" />
		<None Include="..\README.md" Pack="true" PackagePath="\" />
	</ItemGroup>

</Project>

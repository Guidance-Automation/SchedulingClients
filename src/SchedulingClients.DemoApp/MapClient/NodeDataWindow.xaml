<Window x:Class="SchedulingClients.DemoApp.MapClient.NodeDataWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:SchedulingClients.DemoApp.MapClient"
        DataContextChanged="Window_DataContextChanged"
        xmlns:gacoreC="clr-namespace:GACore.Controls;assembly=GACore.Controls"
        xmlns:schedulingClientsCMC="clr-namespace:SchedulingClients.Controls.MapClient;assembly=SchedulingClients.Controls"
        mc:Ignorable="d"
        Title="Node Data" Height="450" Width="800">
    <Window.Resources>
        <ResourceDictionary>
            <ResourceDictionary.MergedDictionaries>
                <ResourceDictionary Source="pack://application:,,,/GACore.Controls;component/ResourceDictionary.xaml" />
            </ResourceDictionary.MergedDictionaries>
        </ResourceDictionary>
    </Window.Resources>
    <DockPanel>
        <gacoreC:LabelContentControl Header="Refresh:" DockPanel.Dock="Top">
            <Button x:Name="refreshButton" Click="refreshButton_Click">Refresh</Button>
        </gacoreC:LabelContentControl>
        <schedulingClientsCMC:NodeDataControl x:Name="nodeDataControl" DockPanel.Dock="Top"/>
    </DockPanel>
</Window>

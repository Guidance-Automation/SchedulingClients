<UserControl x:Class="SchedulingClients.UserControls.JobBuilderClientControl"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
             xmlns:local="clr-namespace:SchedulingClients.UserControls"
             mc:Ignorable="d"
             d:DesignHeight="300" d:DesignWidth="300">
    <Grid>
        <HeaderedContentControl>
            <HeaderedContentControl.Header>
                Job Builder Client Control
            </HeaderedContentControl.Header>
            <StackPanel>
                <Button x:Name="createJobButton" Click="createJobButton_Click">Create Job</Button>
                <Button x:Name="multiPickJobTest" Click="multiPickJobTest_Click">Multi Pick Job Test</Button>
            </StackPanel>
        </HeaderedContentControl>
    </Grid>
</UserControl>
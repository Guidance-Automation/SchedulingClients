<UserControl x:Class="SchedulingClients.Controls.MapClient.GetOccupyingMandateProgressDataControl"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:SchedulingClients.Controls.MapClient"
             mc:Ignorable="d" 
             d:DesignHeight="450" d:DesignWidth="800"
             DataContextChanged="UserControl_DataContextChanged">
    <Grid>
        <StackPanel>
            <StatusBar>Get Occupying Mandate Progress Data</StatusBar>
            <Button x:Name="refreshButton" Click="RefreshButton_Click">Refresh</Button>
            <local:OccupyingMandateProgressDataControl x:Name="occupyingMandateProgressDataControl"/>
        </StackPanel>
    </Grid>
</UserControl>
